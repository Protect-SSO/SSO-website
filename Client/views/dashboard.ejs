<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('../Partials/BootStrap.ejs') %>
</head>
<body>
    <%- include('../Partials/MainNav.ejs') %>
    
    <div class="container">
        <div class="row justify-content-md">
            
            <% for (var i=0; i < Apps.length ; i++) { %>
                <div class="col-md-auto py-4">
        
                    <form action="/Redirect/<%=Apps[i][0]%>"method="get">
                        <div class="card" style="width: 18rem;">
                            <div class="card-body">
                                <h5 class="card-title"><%= Apps[i][0] %></h5>
                                <button class="btn btn-dark" type="submit">Redirect</button>
                            </div>
                        </div>
                    </form>
                </div>
            <% } %>
            <% if(Apps.length == 0) { %>
                <h1></h1><h2 class="h3 mb-3 front-weight-normal">No Apps</h2>
                <% } %>
            
        </div>
    </div>
    

</body>
</html>