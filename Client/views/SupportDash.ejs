<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('../Partials/BootStrap.ejs') %>
</head>
<body>
    <%- include('../Partials/SupportNav.ejs') %>
    
    <div class="container">
        <div class="row justify-content-md">
            
            <% for (var i=0; i < requestedApps.length ; i++) { %>
                <div class="col-md-auto py-4">
        
                    <form action="/AcceptRequest/<%=requestedApps[i][0]%>/<%=requestedApps[i][1]%>"method="post">
                        <div class="card" style="width: 18rem;">
                            <div class="card-body">
                                <h5 class="card-title"><%= requestedApps[i][0] %></h5>
                                <p class="card-text">requested by: <%= requestedApps[i][1] %></p>
                                <button class="btn btn-dark" type="submit">Accept Request</button>
                            </div>
                        </div>
                    </form>
                </div>
            <% } %>

        </div>
    </div>
    
    <% if(requestedApps.length == 0){ %>
        <div text-center mx-auto mt-1 pt-5>
            <h1>No Requests</h1>
        </div>
        
    <% } %>
    
    
</body>
</html>